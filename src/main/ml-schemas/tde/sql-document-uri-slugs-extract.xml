<?xml version="1.0" encoding="UTF-8"?>
<template xmlns="http://marklogic.com/xdmp/tde">
  <path-namespaces>
    <path-namespace>
      <prefix>prop</prefix>
      <namespace-uri>http://marklogic.com/xdmp/property</namespace-uri>
    </path-namespace>
  </path-namespaces>
  <context>prop:properties/identifiers/identifier</context>
  <rows>
    <row>
      <schema-name>documents</schema-name>
      <view-name>compiled_url_slugs</view-name>
      <columns>
	<column>
	  <name>identifier_uuid</name>
	  <scalar-type>string</scalar-type>
	  <val>uuid</val>
	  <reindexing>visible</reindexing>
	</column>
	<column>
	  <name>document_uri</name>
	  <scalar-type>string</scalar-type>
	  <val>xdmp:node-uri(.)</val>
	  <reindexing>visible</reindexing>
	</column>
	<column>
	  <name>identifier_slug</name>
	  <scalar-type>string</scalar-type>
	  <val>url_slug</val>
	  <nullable>false</nullable>
	  <invalid-values>reject</invalid-values>
	  <reindexing>visible</reindexing>
	</column>
	<column>
	  <name>document_published</name>
	  <scalar-type>string</scalar-type>
	  <val>../../published</val>
	  <nullable>false</nullable>
	  <invalid-values>reject</invalid-values>
	  <reindexing>visible</reindexing>
	</column>
      </columns>
    </row>
  </rows>
</template>
